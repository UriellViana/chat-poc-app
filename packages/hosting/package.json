{
  "name": "hosting",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --passWithNoTests --coverage",
    "eject": "react-scripts eject",
    "test:coverage": "yarn test --watchAll",
    "test:ci": "CI=true yarn test",
    "lint:ts": "eslint",
    "lint:css": "stylelint",
    "lint:all:ts": "yarn lint:ts \"src/**/*.{ts,tsx}\"",
    "lint:all:css": "yarn lint:css \"src/**/*.{ts,tsx}\"",
    "lint": "yarn lint:all:ts && yarn lint:all:css",
    "format:all:ts": "yarn format:ts --check \"src/**/*.{ts,tsx}\"",
    "format:ts": "prettier",
    "format": "yarn format:all:ts"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.1",
    "typescript": "3.9.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.3",
    "@testing-library/react": "10.4.8",
    "@testing-library/user-event": "12.1.1",
    "@types/jest": "26.0.10",
    "@types/node": "14.0.27",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "3.9.0",
    "@typescript-eslint/parser": "3.9.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.0.8",
    "prettier": "2.0.5",
    "stylelint": "13.6.1",
    "stylelint-config-recommended": "3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "resetMocks": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/constants.ts",
      "!src/**/index.ts",
      "!**/*.json",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}
